{
  "expo": {
    "name": "rn-soft-ui-kit-free",
    "slug": "rn-soft-ui-kit-free",
    "description": "React Native - Soft UI Kit - Free",
    "primaryColor": "#CB0C9F",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#CB0C9F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#CB0C9F"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.dmgcoding.healthcareappresearch"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "Allow app to open the camera"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b6c72d5b-6f87-4c9b-b509-7ee06df9198d"
      }
    }
  }
}
